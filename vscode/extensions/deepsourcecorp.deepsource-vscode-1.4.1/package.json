{
	"name": "deepsource-vscode",
	"displayName": "DeepSource Autofix™ AI",
	"publisher": "DeepSourceCorp",
	"description": "Improve code quality with static analysis and AI.",
	"repository": {
		"directory": ".",
		"type": "git",
		"url": "https://github.com/DeepSourceCorp/ide-vscode"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Formatters",
		"Machine Learning",
		"Snippets"
	],
	"keywords": [
		"static analysis",
		"autofix",
		"sast",
		"llm",
		"code quality",
		"code review",
		"code analysis",
		"code scanner",
		"deepsource",
		"ai",
		"refactor",
		"codegen",
		"python",
		"javascript",
		"typescript",
		"csharp",
		"java",
		"php",
		"ruby",
		"kotlin",
		"swift",
		"scala"
	],
	"qna": "https://discuss.deepsource.com/",
	"version": "1.4.1",
	"engines": {
		"vscode": "^1.76.0"
	},
	"activationEvents": [
		"*"
	],
	"main": "out/extension.js",
	"icon": "resources/icon.png",
	"contributes": {
		"walkthroughs": [
			{
				"id": "deepsource.walkthrough",
				"title": "Get started with DeepSource Autofix™ AI",
				"description": "Find and fix problems in your code effortlessly.",
				"steps": [
					{
						"id": "login",
						"title": "Log in with DeepSource",
						"description": "Connect VS Code with your DeepSource Cloud or Enterprise account. Signing up is free and takes only a few seconds.\n\n [Connect with DeepSource](command:deepsource.triggerLogin)",
						"media": {
							"image": "resources/walkthrough/1_login.png",
							"altText": "Login with a DeepSource account"
						}
					},
					{
						"id": "start-analysis",
						"title": "Start analysis on a repository",
						"description": "Open a repository to get started. You can choose which analyzers should run on your code. We also recommend adding a `.deepsource.toml` file for more control.",
						"media": {
							"image": "resources/walkthrough/2_analyzers.gif",
							"altText": "Run a DeepSource analysis"
						}
					},
					{
						"id": "view-issues",
						"title": "See detected issues in real-time",
						"description": "You're all set. Every time you write code and save a file, DeepSource automatically analyzes and raises issues from thousands of checkers. Look for those yellow squiggly lines!",
						"media": {
							"image": "resources/walkthrough/3_explain.gif",
							"altText": "View issues and suggestions"
						}
					},
					{
						"id": "autofix-issues",
						"title": "Autofix™ your first issue",
						"description": "Automatically generate fixes for any issue DeepSource raises. You'll see a diff of the changes that you can accept or discard.",
						"media": {
							"image": "resources/walkthrough/4_autofix.gif",
							"altText": "Fix issues with Autofix AI"
						}
					},
					{
						"id": "feedback",
						"title": "Tell us what you think!",
						"description": "Got feedback or questions for us? Look for the “Share feedback…” button and let us know what you're thinking.",
						"media": {
							"image": "resources/walkthrough/5_feedback.gif",
							"altText": "Share feedback"
						}
					}
				]
			}
		],
		"viewsWelcome": [
			{
				"view": "activate-deepsource",
				"contents": "WELCOME TO DEEPSOURCE\nDeepSource is a code health platform that helps you ship good code. Connect to your DeepSource dashboard to start analyzing your local code.\n\n[Connect to DeepSource](command:deepsource.activateRepository)\n\n[Read the docs](https://docs.deepsource.com/docs) to learn more about the VS code plugin."
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "deepsource",
					"title": "DeepSource",
					"icon": "resources/logo.svg"
				}
			]
		},
		"views": {
			"deepsource": [
				{
					"type": "webview",
					"id": "deepsource.welcomePage",
					"name": "DeepSource",
					"when": "deepsource.initState == 2 && deepsource.isNewUser"
				},
				{
					"type": "webview",
					"id": "deepsource.sidebar",
					"name": "Issues in this file",
					"when": "!deepsource.isNewUser"
				},
				{
					"type": "webview",
					"id": "deepsource.affectedFiles",
					"name": "Affected Files",
					"when": "deepsource.initState > 2 && deepsource.initState < 8"
				},
				{
					"type": "webview",
					"id": "deepsource.analyzers",
					"name": "Analyzers",
					"when": "deepsource.initState > 2 && deepsource.initState < 8"
				},
				{
					"type": "webview",
					"id": "deepsource.settings",
					"name": "Settings",
					"when": "deepsource.initState > 2 && deepsource.initState < 8"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "deepsource.fetchNewIssues",
					"when": "(view == deepsource.sidebar || view == deepsource.affectedFiles) && !orphanMode && deepsource.initState > 2 && deepsource.initState < 8 && showingExistingIssues",
					"group": "navigation"
				},
				{
					"command": "deepsource.fetchExistingIssues",
					"when": "(view == deepsource.sidebar || view == deepsource.affectedFiles) && !orphanMode && deepsource.initState > 2 && deepsource.initState < 8 && !showingExistingIssues",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
				"command": "deepsource.fetchExistingIssues",
				"title": "Issues on baseline branch",
				"icon": "$(debug-step-back)",
				"enablement": "!showingExistingIssues",
				"shortTitle": "Show issues on default branch"
			},
			{
				"command": "deepsource.fetchNewIssues",
				"title": "Issues on this branch",
				"icon": "$(debug-step-over)",
				"enablement": "showingExistingIssues",
				"shortTitle": "Show issues on this branch"
			},
			{
				"command": "deepsource.startAnalysis",
				"title": "DeepSource: Run analysis",
				"icon": "$(play)"
			},
			{
				"command": "deepsource.stopAnalysis",
				"title": "DeepSource: Stop analysis",
				"icon": "$(stop)"
			},
			{
				"command": "deepsource.previewConfig",
				"title": "DeepSource: View config"
			},
			{
				"command": "deepsource.logOut",
				"title": "DeepSource: Log out"
			},
			{
				"command": "deepsource.welcome",
				"title": "DeepSource: Welcome"
			},
			{
				"command": "deepsource.generateSupportBundle",
				"title": "DeepSource: Generate support bundle",
				"icon": "$(file-zip)"
			}
		],
		"keybindings": [
			{
				"command": "deepsource.startAnalysis",
				"when": "deepsource.initState > 2 && deepsource.initState < 8 && editorTextFocus",
				"key": "Alt+[KeyD] [KeyA]"
			},
			{
				"command": "deepsource.stopAnalysis",
				"when": "deepsource.initState > 2 && deepsource.initState < 8 && editorTextFocus",
				"key": "Alt+[KeyD] [KeyS]"
			},
			{
				"command": "deepsource.previewConfig",
				"when": "deepsource.initState > 2 && deepsource.initState < 8",
				"key": "Alt+[KeyD] [KeyV]"
			},
			{
				"command": "deepsource.showIssueDescription",
				"when": "deepsource.initState > 2 && deepsource.initState < 8 && editorTextFocus",
				"key": "Alt+[KeyD] [KeyE]"
			},
			{
				"command": "deepsource.triggerAutofix",
				"when": "deepsource.hasAutofixAccess && deepsource.initState > 2 && deepsource.initState < 8 && deepsource.autofixState != 'working' && deepsource.autofixState != 'waiting' && editorTextFocus",
				"key": "Alt+[KeyD] [KeyF]"
			},
			{
				"command": "deepsource.autofix.accept",
				"when": "deepsource.hasAutofixAccess && deepsource.initState > 2 && deepsource.initState < 8 && deepsource.autofixState == 'waiting' && editorTextFocus",
				"key": "Alt+[KeyD] [KeyY]"
			},
			{
				"command": "deepsource.autofix.reject",
				"when": "deepsource.hasAutofixAccess && deepsource.initState > 2 && deepsource.initState < 8 && deepsource.autofixState == 'waiting' && editorTextFocus",
				"key": "Alt+[KeyD] [KeyN]"
			},
			{
				"command": "deepsource.autofix.cancel",
				"when": "deepsource.hasAutofixAccess && deepsource.initState > 2 && deepsource.initState < 8 && deepsource.autofixState == 'working' && editorTextFocus",
				"key": "Alt+[KeyD] [KeyC]"
			}
		],
		"configuration": {
			"title": "DeepSource",
			"properties": {
				"deepsource.installationUrl": {
					"type": "string",
					"default": "https://app.deepsource.com",
					"description": "Your DeepSource installation's host URL, if you're on Enterprise Server"
				},
				"deepsource.highlightAffectedLines": {
					"type": "boolean",
					"default": false,
					"description": "When enabled, DeepSource issues will be highlighted in the editor"
				},
				"deepsource.showCodelens": {
					"type": "boolean",
					"default": true,
					"description": "When enabled, DeepSource codelens will be shown in the editor"
				}
			}
		}
	},
	"extensionDependencies": [
		"vscode.git"
	],
	"scripts": {
		"vscode:prepublish": "yarn build",
		"ext:build": "yarn ext:proto && NODE_ENV=production node bundle.mjs",
		"ext:watch": "node bundle.mjs",
		"ext:proto": "protoc --ts_out ./src/rpc --ts_opt optimize_speed --proto_path src/rpc src/rpc/*.proto",
		"ui:auth:watch": "PAGE=auth vite build -w",
		"ui:auth:build": "PAGE=auth vite build",
		"ui:error:watch": "PAGE=error vite build -w",
		"ui:error:build": "PAGE=error vite build",
		"ui:init:watch": "PAGE=init vite build -w",
		"ui:init:build": "PAGE=init vite build",
		"ui:happy:watch": "PAGE=happy vite build -w",
		"ui:happy:build": "PAGE=happy vite build",
		"ui:welcome:watch": "PAGE=welcome vite build -w",
		"ui:welcome:build": "PAGE=welcome vite build",
		"ui:settings:watch": "PAGE=settings vite build -w",
		"ui:settings:build": "PAGE=settings vite build",
		"ui:affected-files:build": "PAGE=affected-files vite build",
		"ui:affected-files:watch": "PAGE=affected-files vite build -w",
		"ui:issue-description:watch": "PAGE=issue-description vite build -w",
		"ui:issue-description:build": "PAGE=issue-description vite build",
		"ui:analyzers:watch": "PAGE=analyzers vite build -w",
		"ui:analyzers:build": "PAGE=analyzers vite build",
		"dev": "concurrently \"npm:*:watch\"",
		"build": "concurrently \"npm:ui:*:build\" && yarn ext:build",
		"package": "NODE_ENV=production vsce package --yarn --allow-star-activation --allow-missing-repository"
	},
	"devDependencies": {
		"@types/markdown-it": "^12.2.3",
		"@types/node": "16.x",
		"@types/prismjs": "^1.26.0",
		"@types/semver": "^7.5.0",
		"@types/vscode": "^1.76.0",
		"@types/vscode-webview": "^1.57.1",
		"@vscode/vsce": "^2.20.1",
		"concurrently": "^8.2.0",
		"esbuild": "^0.18.6",
		"terser": "^5.18.1",
		"typescript": "^5",
		"vite": "^4.3.9",
		"vite-plugin-singlefile": "^0.13.5",
		"vite-plugin-solid": "^2.7.0"
	},
	"dependencies": {
		"@flatten-js/interval-tree": "^1.0.21",
		"@grpc/grpc-js": "^1.8.15",
		"@iarna/toml": "^2.2.5",
		"@protobuf-ts/grpc-transport": "^2.9.0",
		"@protobuf-ts/plugin": "^2.9.0",
		"@vscode/codicons": "^0.0.33",
		"@vscode/webview-ui-toolkit": "^1.2.2",
		"immer": "^10.0.2",
		"lookpath": "^1.2.2",
		"markdown-it": "^13.0.1",
		"posthog-node": "^3.1.3",
		"prismjs": "^1.29.0",
		"quick-lru": "^6.1.1",
		"semver": "^7.5.4",
		"solid-js": "^1.7.6",
		"toml": "^3.0.0"
	},
	"config": {
		"protocVersion": "26.1"
	},
